   "source": [
    "#Input bilangan prima ke-N\n",
    "\n",
    "def prima_ke(n):\n",
    "    val = 3\n",
    "    k = 1\n",
    "    while k<n:\n",
    "        if all(val%i!=0 for i in range(2,val)):\n",
    "            k += 1\n",
    "        val += 1\n",
    "    return val-1\n",
    "\n",
    "n = int(input())\n",
    "print('Bilangan prima ke-%i adalah %i'%(n,prima_ke(n)))"
   ]
  }
