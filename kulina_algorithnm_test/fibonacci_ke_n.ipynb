   "source": [
    "#Input bilangan Fibonaci ke-N\n",
    "\n",
    "def fibo(n):\n",
    "    if n==0:\n",
    "        return 0\n",
    "    elif n==1:\n",
    "        return 1\n",
    "    else:\n",
    "        return fibo(n-1) + fibo(n-2)\n",
    "        \n",
    "n = int(input())\n",
    "print('Bilangan fibonaci ke-%i adalah %i'%(n,fibo(n)))"
   ]
  }
