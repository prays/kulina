    "#Input suatu bilangan maka akan muncul digit satuan, puluhan, ratusan, dan seterusnya dari setiap digit bilangan tersebut\n",
    "def digit(n):\n",
    "    length = len(n)\n",
    "    for i in range(0,length):\n",
    "        print(n[i] + '0' * (length-1))\n",
    "        length -= 1\n",
    "\n",
    "n = input()\n",
    "digit(n)"
   ]
  }
